<h2>Show User</h2>
<ul>

  <li>
    <strong>Email:</strong>
    <%= @user.email %>
  </li>

  <li>
    <strong>Name:</strong>
    <%= @user.name %>
  </li>

  <li>
    <strong>Managers:</strong>
    <%= Enum.join(@managers, ", ") %>
  </li>

  <li>
    <strong>Manages:</strong>
    <%= Enum.join(@managees, ", ") %>
  </li>

</ul>

<span><%= link "Edit", to: user_path(@conn, :edit, @user) %></span>
<span><%= link "Back", to: user_path(@conn, :index) %></span>
